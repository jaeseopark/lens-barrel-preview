{
  "name": "lens-barrel-preview",
  "version": "0.0.1",
  "description": "A TypeScript library for rendering top-down camera and lens preview images using SVG",
  "main": "dist/lens-barrel-preview.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "clean": "rm -rf dist",
    "dev": "webpack --mode development --watch",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "camera",
    "lens",
    "preview",
    "visualization",
    "photography",
    "typescript"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaeseopark/lens-barrel-preview"
  },
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "copy-webpack-plugin": "^12.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  }
}
